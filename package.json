{
  "name": "serverless-python-gitlab-ci",
  "version": "1.0.0",
  "description": "Boilerplate for a python serverless lambda setup",
  "main": "index.js",
  "author": "bogdandrema@gmail.com",
  "license": "MIT",
  "scripts": {
    "lint:eslint": "eslint --fix",
    "lint:stylelint": "stylelint --fix",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "yarn lint:eslint --ext .js .",
    "lint:all:markdownlint": "yarn lint:markdownlint \"docs/*.md\" \"*.md\"",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,md}\"",
    "lint": "run-s lint:all:*",
    "e2e": "cypress run",
    "verify": "cypress verify"
  },
  "dependencies": {
    "@anttiviljami/serverless-stack-output": "0.3.1",
    "serverless-puresec-cli": "1.2.0",
    "serverless-python-requirements": "4.3.0"
  },
  "devDependencies": {
    "eslint": "5.12.0",
    "eslint-config-prettier": "3.5.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-ava": "6.0.0",
    "eslint-plugin-cypress": "2.2.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "markdownlint-cli": "0.13.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.15.3"
  }
}
